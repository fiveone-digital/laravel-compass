{
    "name": "fiveone-digital/laravel-compass",
    "description": "An elegant REST assistent for the Laravel framework.",
    "keywords": [
        "laravel",
        "API",
        "testing",
        "documentation"
    ],
    "homepage": "https://github.com/davidhsianturi/laravel-compass",
    "license": "MIT",
    "authors": [
        {
            "name": "David H. Sianturi",
            "email": "davidhsianturi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.21",
        "illuminate/routing": "^8.0|^9.0",
        "illuminate/support": "^9.21",
        "fiveone-digital/documentarian": "@dev"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.5",
        "laravel/sanctum": "^2.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Davidhsianturi\\Compass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Davidhsianturi\\Compass\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Davidhsianturi\\Compass\\CompassServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "fiveone-digital/documentarian": {
            "type": "vcs",
            "url": "git@github.com:fiveone-digital/documentarian.git"
        }
    }
}
